{
  "paths": {
    "data_dir": "../data/gaoyayang",
    "bids_dir": null,
    "output_dir": "../result"
  },
  "preprocessing": {
    "resample_hz": 250,
    "bandpass": {
      "l_freq": 1,
      "h_freq": 40
    },
    "notch": {
      "freqs": [
        50
      ]
    },
    "montage": {
      "name": "standard_1020"
    },
    "reference": {
      "kind": "none"
    }
  },
  "power": {
    "bands": {
      "delta": [1, 4],
      "theta": [4, 8],
      "alpha": [8, 13],
      "beta": [13, 30]
    },
    "welch": {
      "n_fft": 256,
      "n_overlap": 0,
      "n_per_seg": null
    }
  },
  "entropy": {
    "permutation": {
      "bands": {
        "full": [1, 40]
      },
      "order": 3,
      "delay": 1,
      "normalize": true
    },
    "spectral": {
      "band_label": "full",
      "method": "fft",
      "nperseg": null,
      "normalize": true
    }
  },
  "Segment": {
    "Segment_length": 60,
    "bad_segment_tolerance": 0.5
  },
  "report": {
    "title": "qEEG QC Report",
    "author": "Data Ops"
  }
}
